function changenav(nav){
	if (document.getElementById("nav_1")!=null)
		document.getElementById("nav_1").className = "";
	if (document.getElementById("nav_2")!=null)
		document.getElementById("nav_2").className = "";
	if (document.getElementById("nav_3")!=null)
		document.getElementById("nav_3").className = "";
	if (document.getElementById("nav_4")!=null)
		document.getElementById("nav_4").className = "";
	if (document.getElementById("nav_5")!=null)
		document.getElementById("nav_5").className = "";
	if (document.getElementById("nav_6")!=null)
		document.getElementById("nav_6").className = "";
		document.getElementById(nav).className = "current";
		document.getElementById("sub_nav_1").style.display = "none";
		document.getElementById("sub_nav_2").style.display = "none";
		document.getElementById("sub_nav_3").style.display = "none";
		document.getElementById("sub_nav_4").style.display = "none";
		document.getElementById("sub_nav_5").style.display = "none";
		document.getElementById("sub_nav_6").style.display = "none";
		document.getElementById("sub_"+nav).style.display = "";		
	}